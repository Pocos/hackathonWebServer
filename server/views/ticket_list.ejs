<h2>Ticket List</h2>
<table border=4>
	<thead>
		<tr>
			<td>Device Id</td>
			<td>Timestamp</td>
			<td></td>
		</thead>
		<tr ng-repeat="ticket in ticket_list track by $index">
			<td>{{ticket.device_id}}</td>
			<td>{{ticket.timestamp}}</td>
			<td><a href="#/user_gcm/{{ticket.device_id}}">Open Console</a></td>
		</td></tr></table>
		Automatic Refresh: <input type="checkbox" ng-model="refresh" ng-change="change()"> <br/>